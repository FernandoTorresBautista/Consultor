extends layout
block content
	nav
		p Usuario
		br  
		li
			a.active(href="./oui", class="active") Ui
		|   
		li
			a(href="./oquery") Consultas
		|   
		li
			a(href="./oviews") Listar views
		|   
		li
			a(href="./odocument") Documentos
		|   
		li
			a(href="./odb") Conexiones
		|   
		li
			a(href="./ologout") Cerrar sesión
		|   
	nav
		li
			a(href='./oadd_ui') Agregar ui
		|   
		li
			a(href='./odel_ui') Eliminar ui
		|   
	nav
		br
		p Lista de ui del uis
		table(border='1')
			thead
				tr
					th id
					th name
					th descripción
					th email
					th created_at
			tbody
				each ui in uis
					tr
						td=ui.id
						td=ui.name
						td=ui.description
						td=ui.email
						td=ui.created_at
	br


